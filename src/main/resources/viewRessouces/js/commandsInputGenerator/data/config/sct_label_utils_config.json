                                {
                                    "HTMLType"    : "checkbox",
                                    "Title"       : " dilate"
                                },
                                "mandatory"   : "false"
                            },
							
                            {
                                "command"      : "-v",
                                "id"           : "sct_label_utils_verbose_ID",
                                "description"  : "display on",
                                "HTMLRendering"		   :
                                {
                                    "HTMLType"    : "checkbox",
                                    "Title"       : "Display on"
                                },
                                "mandatory"   : "false"
                            },
                            {
                                "command"      : "-h",
                                "id"           : "sct_label_utils_help_ID",
                                "description"  : "Help",
                                "HTMLRendering"		   :
                                {
                                    "HTMLType"    : "checkbox",
                                    "Title"       : "Help"
                                },
                                "mandatory"   : "false"
                            }
                        ]
